{
	"name": "all1n.dev",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"node": ">=24.0.0",
		"npm": ">=11.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"generate-resume-html": "resumed render --theme jsonresume-theme-even --output=public/resume.html public/resume.json",
		"generate-resume-pdf": "resumed export --theme jsonresume-theme-even --output=public/resume.pdf public/resume.json",
		"generate-resume": "npm run generate-resume-html && npm run generate-resume-pdf",
		"build": "astro check && tsc --noEmit && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"prepare": "husky",
		"postinstall": "npx tsx scripts/stats-fetch.ts"
	},
	"dependencies": {
		"@allindevelopers/prettier-config": "^1.0.0",
		"@astrojs/check": "^0.9.5",
		"@astrojs/react": "^4.4.1",
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@playform/inline": "^0.1.2",
		"@tailwindcss/vite": "^4.1.16",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"astro": "^5.15.3",
		"cheerio": "^1.1.2",
		"domhandler": "^5.0.3",
		"husky": "^9.1.7",
		"jsonresume-theme-even": "^0.26.1",
		"lint-staged": "^16.2.6",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"puppeteer": "24.10.1",
		"react": "^19.2.0",
		"react-activity-calendar": "^2.7.15",
		"react-dom": "^19.2.0",
		"resumed": "^6.1.0",
		"tailwindcss": "^4.1.4",
		"tailwindcss-animated": "^2.0.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	},
	"lint-staged": {
		"scripts/contributions/*.json": "npx tsx scripts/l10n-sort.ts",
		"*": "npx prettier --write --ignore-unknown"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
